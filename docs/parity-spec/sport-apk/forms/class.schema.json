      "minimum": 0
    },
    "grade_scheme": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "is_dirty": {
      "type": "number"
    },
    "name": {
      "type": "string"
    },
    "school_year": {
      "type": "string"
    },
    "settings": {
      "type": "object"
    },
    "sort": {
      "type": "string"
    },
    "stats": {
      "type": "object"
    },
    "student_count": {
      "type": "number",
      "minimum": 0
    },
    "teacher_id": {
      "type": "string"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    },
    "version": {
      "type": "number"
    }
  },
  "required": [
